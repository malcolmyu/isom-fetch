{
  "name": "isom-fetch",
  "version": "1.0.2",
  "description": "A isomorphic fetch for SSR and koa ",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "server:dev": "better-npm-run server:dev",
    "karma:dev": "better-npm-run karma:dev",
    "karma:travis": "better-npm-run karma:travis",
    "test:dev": "npm run build && concurrently --kill-others \"npm run server:dev\" \"npm run karma:dev\"",
    "test": "npm run build && concurrently --raw --kill-others --success \"npm run server:dev\" \"npm run karma:travis\"",
    "lint": "eslint src",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "karma:dev": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "karma:travis": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "travis"
      }
    },
    "build": {
      "command": "npm run clean && babel src --out-dir lib",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "commonjs"
      }
    },
    "server:dev": {
      "command": "node test/_server.babel.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malcolmyu/isom-fetch.git"
  },
  "keywords": [
    "isomorphic",
    "fetch",
    "SSR",
    "koa"
  ],
  "author": "malcolmyu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malcolmyu/isom-fetch/issues"
  },
  "homepage": "https://github.com/malcolmyu/isom-fetch#readme",
  "dependencies": {
    "axios": "^0.12.0",
    "es6-promise": "^3.2.1",
    "invariant": "^2.2.1",
    "process": "^0.11.3"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "co": "^4.6.0",
    "concurrently": "^2.1.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "koa": "^1.2.0",
    "koa-bodyparser": "^2.2.0",
    "koa-cors": "0.0.16",
    "koa-router": "^5.4.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1"
  }
}
